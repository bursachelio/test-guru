<tr>
  <td><%= test.title %></td>
  <td><%= test.questions.count %></td>
  <td>
    <%= link_to 'Просмотр', test_path(test) %>
    <%= link_to 'Редактировать', edit_test_path(test) %>
    <%= button_to 'Удалить', test_path(test), method: :delete, data: { confirm: 'Вы уверены?' } %>
    <td><%= button_to 'Начать', start_test_path(test) %></td>
  </td>
</tr>
